{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Run SchatzApp",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build-SchatzApp",
        "program": "${workspaceFolder}/SchatzApp/bin/Debug/netcoreapp3.1/SchatzApp.dll",
        "args": [],
        "cwd": "${workspaceFolder}/SchatzApp",
        "console": "integratedTerminal",
        "stopAtEntry": false,
        "launchBrowser": { "enabled": true },
        "launchUrl": "http://localhost:5001",
        "serverReadyAction": {
          "action": "openExternally",
          "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
        },
        "environmentVariables": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        }
      }
    ]
  }
  